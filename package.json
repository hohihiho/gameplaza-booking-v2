    "dev:with-browser": "./scripts/dev-with-browser.sh",
    "prebuild": "git config --global user.name 'hohihu' 2>/dev/null || true && git config --global user.email 'ndz5496@gmail.com' 2>/dev/null || true && git config --global init.defaultBranch main 2>/dev/null || true",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc -p tsconfig.app.json --noEmit",
    "type-check:all": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
