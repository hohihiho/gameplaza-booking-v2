  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "dev:turbo": "next dev --turbopack -H 0.0.0.0",
    "dev:restart": "./scripts/restart-dev.sh",
    "dev:clean": "./scripts/restart-dev.sh --clean",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:realdb": "jest --selectProjects=realdb",
    "test:realdb:watch": "jest --selectProjects=realdb --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "generate-test-data": "tsx scripts/create-analytics-test-data.ts",
    "generate-real-data": "tsx scripts/create-real-world-analytics-data.ts",
    "seed": "tsx scripts/seed-database.ts all",
    "seed:superadmin": "tsx scripts/seed-database.ts superadmin",
    "test:e2e": "playwright test",
    "test:e2e:safe": "./scripts/test-e2e-safe.sh",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:mobile": "playwright test tests/e2e/specs/mobile.spec.ts",
    "test:e2e:performance": "playwright test --config=playwright-performance.config.ts",
    "test:puppeteer": "jest tests/puppeteer --testTimeout=30000",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:k6": "k6 run tests/k6/load-test-scenarios.js",
    "test:k6:spike": "K6_SCENARIO=spike_test k6 run tests/k6/load-test-scenarios.js",
    "test:k6:concurrent": "K6_SCENARIO=concurrent_reservations k6 run tests/k6/load-test-scenarios.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:puppeteer && npm run test:cypress && npm run test:k6",
    "test:quick": "npm run test:unit && npm run test:integration",
    "test:ui-only": "npm run test:e2e && npm run test:puppeteer && npm run test:cypress",
    "test:performance": "npm run test:k6",
    "test:report": "npm run test:coverage && npm run test:e2e && open coverage/lcov-report/index.html",
    "security:check": "tsx scripts/security-check.ts",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:full": "npm run security:check && npm run security:audit",
    "logs": "tsx scripts/check-logs.ts",
    "logs:watch": "tsx scripts/watch-logs.ts",
    "logs:errors": "tsx scripts/check-logs.ts --level=error",
    "logs:api": "tsx scripts/check-logs.ts --source=api",
    "logs:recent": "tsx scripts/check-logs.ts --recent=5m"
  },
