    "test:scan:errors": "playwright test -c playwright-errors.config.ts",
    "scan": "playwright test -c playwright-errors.config.ts",
    "scan:open": "playwright test -c playwright-errors.config.ts; playwright show-report",
    "scan:fast": "ERROR_SCAN_MAX_PAGES=10 playwright test -c playwright-errors.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:pages": "playwright test tests/e2e/specs/site-pages.spec.ts",
    "test:e2e:auth": "playwright test tests/e2e/specs/site-pages-auth.spec.ts",
    "test:e2e:ranking": "playwright test tests/e2e/specs/ranking.spec.ts",
    "verify:public": "tsx scripts/verify-public-data.ts",
    "test:e2e:mobile": "playwright test tests/e2e/specs/mobile.spec.ts",
    "seed": "tsx scripts/seed-database.ts all",
    "seed:superadmin": "tsx scripts/seed-database.ts superadmin",
    "backup": "node scripts/backup-database.js",
    "backup:status": "node scripts/backup-database.js status",
    "restore": "node scripts/restore-database.js",
    "fix": "./scripts/quick-fix.sh",
    "fix:port": "./scripts/quick-fix.sh port",
    "dev:cf": "D1_ENABLED=true npx @cloudflare/next-on-pages@latest --watch",
    "db-api:dev": "cd workers/db-api && npx wrangler dev",
    "db-api:deploy": "cd workers/db-api && npx wrangler deploy",
    "db-api:deploy:prod": "cd workers/db-api && npx wrangler deploy --env production",
    "db-api:push-schema:dev": "cd workers/db-api && npx wrangler d1 execute gameplaza-development --file=../../docs/sql/d1_reservations_and_pricing.sql",
    "db-api:push-schema:prod": "cd workers/db-api && npx wrangler d1 execute gameplaza-production --file=../../docs/sql/d1_reservations_and_pricing.sql",
    "db-api:push-device-catalog:dev": "cd workers/db-api && npx wrangler d1 execute gameplaza-development --file=../../docs/sql/d1_device_catalog.sql",
    "db-api:push-device-catalog:prod": "cd workers/db-api && npx wrangler d1 execute gameplaza-production --file=../../docs/sql/d1_device_catalog.sql",
    "db-api:seed-cms:dev": "cd workers/db-api && npx wrangler d1 execute gameplaza-development --file=../../migrations/2025-09-14_003_cms_seed.sql",
    "db-api:seed-cms:prod": "cd workers/db-api && npx wrangler d1 execute gameplaza-production --file=../../migrations/2025-09-14_003_cms_seed.sql"
  },
